<config>
  <!-- Client Control -->
  <fold-anon v='true'/>

  <!-- Folding Slot Configuration -->
  <gpu v='false'/>

  <!-- HTTP Server -->
  #Following allows access from local network
  <allow v='192.168.1.1/24'/>

  <!-- Remote Command Server -->
  <password v='PASSWORD'/>

  <!-- User Information -->
  #Change Team Number and Username if desired
  <team v='0'/>
  <user v='anonymous'/>

  <!-- Web Server -->
  #Following allows access from local network
  <web-allow v='192.168.1.1/24'/>

  <!-- Folding Slots -->
  <slot id='0' type='CPU'/>
</config>